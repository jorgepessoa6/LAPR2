<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="700.0" prefWidth="950.0"
            stylesheets="@../../css/usecases.css" xmlns="http://javafx.com/javafx/8"
            fx:controller="lapr2.agpsdapplication.controller.humanresourcesofficer.ViewEvaluationsPage">
    <children>
        <TableView fx:id="tableViewEvaluations" AnchorPane.bottomAnchor="30.0" AnchorPane.leftAnchor="30.0"
                   AnchorPane.rightAnchor="30.0" AnchorPane.topAnchor="30.0">
            <columns>
                <TableColumn fx:id="tableColumnNumber" editable="false" maxWidth="200.0" minWidth="200.0"
                             prefWidth="200.0"
                             resizable="false" text="Mechanographical number"/>
                <TableColumn fx:id="tableColumnName" editable="false" maxWidth="350.0" minWidth="350.0"
                             prefWidth="350.0" resizable="false" text="Name"/>
                <TableColumn fx:id="tableColumnLabel" maxWidth="250.0" minWidth="250.0" prefWidth="250.0"
                             resizable="false" text="Rating label"/>
                <TableColumn fx:id="tableColumnRating" maxWidth="90.0" minWidth="90.0" prefWidth="90.0"
                             resizable="false" text="Rating"/>
            </columns>
            <columnResizePolicy>
                <TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/>
            </columnResizePolicy>
        </TableView>
    </children>
</AnchorPane>
