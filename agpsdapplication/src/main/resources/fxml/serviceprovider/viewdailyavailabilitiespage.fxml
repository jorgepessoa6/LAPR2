<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="700.0" prefWidth="950.0"
            stylesheets="@../../css/usecases.css" xmlns="http://javafx.com/javafx/8"
            fx:controller="lapr2.agpsdapplication.controller.serviceprovider.ViewDailyAvailabilitiesPage">
    <padding>
        <Insets bottom="30.0" left="30.0" right="30.0" top="30.0"/>
    </padding>
    <TableView fx:id="tableView" prefHeight="700.0" prefWidth="950.0" AnchorPane.bottomAnchor="0.0"
               AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
        <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/>
        </columnResizePolicy>
        <columns>
            <TableColumn fx:id="tableColumnDate" editable="false" maxWidth="290.0" minWidth="290.0"
                         prefWidth="290.0" resizable="false" sortable="false" text="Date"/>
            <TableColumn fx:id="tableColumnStartTime" editable="false" maxWidth="300.0" minWidth="300.0"
                         prefWidth="300.0" resizable="false" sortable="false" text="Start time"/>
            <TableColumn fx:id="tableColumnEndTime" editable="false" maxWidth="300.0" minWidth="300.0"
                         prefWidth="300.0" resizable="false" sortable="false" text="End time"/>
        </columns>
    </TableView>
</AnchorPane>
